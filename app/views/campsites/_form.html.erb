<%= simple_form_for @campsite, data: { target: 'campsite-form.form' } do |f| %> 
  <%= f.input :name %> 
  <%= f.input :address %> 
  <%= f.input :postal_code %> 
  <%= f.input :county %> 
  <%= f.input :area %> 
  <%= f.input :category %> 
  <%= f.input :website %> 
  <%= f.input :phone %> 
  <%= f.input :description %> 
  <br> 
  <%= f.button :submit, class: "btn btn-green-dark" %> 
<% end %>